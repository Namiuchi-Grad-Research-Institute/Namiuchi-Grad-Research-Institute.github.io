{
    "compilerOptions": {
        "jsx": "preserve",
        "module": "esnext",
        "strictFunctionTypes": true,
        "target": "esnext"
    },
    "exclude": [
        "**/node_modules/*",
        "node_modules"
    ],
    "include": [
        "components/**/*.js",
        "components/**/*.jsx",
        "hooks/**/*.js",
        "hooks/**/*.jsx",
        "lib/**/*.js",
        "lib/**/*.jsx",
        "pages/**/*.js",
        "pages/**/*.jsx"
    ]
}
